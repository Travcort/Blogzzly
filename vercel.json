{
    "version": 2,
    "buildCommand": "sh build_script.sh",
    "builds": [
      {
        "src": "./index.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.11" 
        }
      }
    ],
    "rewrites": [
      { 
        "source": "/(.*)", 
        "destination": "./index.py" 
      }
    ]
}