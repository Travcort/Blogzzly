{
    "version": 2,
    "buildCommand": "sh build_script.sh",
    "builds": [
      {
        "src": "/api/index.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.11" 
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.py"
      }
    ]
}